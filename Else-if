public class Program {
    public static void main(String[] args) {
        int age = 25;
        if (age<=0){
            System.out.println("error");
        }else if(age<10){
            System .out.println("too young");
        }else if(age<100){
            System.out.println("welcome");
        }else{
            System.out.println("something is wrong");
        }
}
}
